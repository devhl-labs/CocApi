<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.1</TargetFramework>
		<LangVersion>8.0</LangVersion>
		<Nullable>enable</Nullable>
		<Authors>devhl</Authors>
		<Product />
		<Description>This is a Clash of Clans library that will return respsonses from developer.clashofclans.com</Description>
		<Version>0.3.0-alpha</Version>
		<AssemblyVersion>0.3.0.0</AssemblyVersion>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageProjectUrl>https://github.com/devhl-labs/CocApi</PackageProjectUrl>
		<PackageId>CocApiLibrary</PackageId>
		<Copyright>2019</Copyright>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageIconUrl>http://devhl.com/CocApi.ico</PackageIconUrl>
		<RepositoryUrl>https://github.com/devhl-labs/CocApi</RepositoryUrl>
		<PackageTags>Clash Clans ClashOfClans ClashClans SuperCell devhl</PackageTags>
		<PackageReleaseNotes>Initial Release</PackageReleaseNotes>
		<FileVersion>0.3.0.0</FileVersion>
	</PropertyGroup>

	<ItemGroup>
		<Compile Remove="jsonconverterattribute.cs" />
		<Compile Remove="SCDateTimeConverterAttribute.cs" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="System.Net.Http" Version="4.3.4" />
		<PackageReference Include="System.Net.Requests" Version="4.3.0" />
		<PackageReference Include="System.Reflection.Emit.Lightweight" Version="5.0.0-alpha1.19473.2" />
		<PackageReference Include="System.Text.Json" Version="5.0.0-alpha1.19473.2" />
	</ItemGroup>

</Project>
