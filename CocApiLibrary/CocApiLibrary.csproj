<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
	<LangVersion>8.0</LangVersion>
	<Nullable>enable</Nullable>
	<Authors>devhl</Authors>
	<Product />
	<Description>This is a Clash of Clans library that will return respsonses from developer.clashofclans.com</Description>
	<Version>0.0.1</Version>
	<AssemblyVersion>0.0.1.0</AssemblyVersion>
	<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
	<PackageProjectUrl>https://github.com/devhl-labs/CocApi</PackageProjectUrl>
	<PackageId>CocApiLibrary</PackageId>
	<Copyright>2019</Copyright>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="jsonconverterattribute.cs" />
    <Compile Remove="SCDateTimeConverterAttribute.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="System.Net.Http" Version="4.3.4" />
    <PackageReference Include="System.Net.Requests" Version="4.3.0" />
    <PackageReference Include="System.Reflection.Emit.Lightweight" Version="5.0.0-alpha1.19380.5" />
    <PackageReference Include="System.Text.Json" Version="5.0.0-alpha1.19380.5" />
  </ItemGroup>

</Project>
