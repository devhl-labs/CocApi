<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <LangVersion>8.0</LangVersion>
    <Nullable>enable</Nullable>
    <Authors>devhl</Authors>
    <Product />
    <Description>A wrapper for the Clash of Clans API.</Description>
    <Version>1.3.2.0-preview.1.0.0.1</Version>
    <AssemblyVersion>1.3.2.0</AssemblyVersion>
    <FileVersion>1.3.2.0</FileVersion>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageProjectUrl>https://discord.gg/AT4wJNq</PackageProjectUrl>
    <PackageId>CocApi</PackageId>
    <Copyright>2020</Copyright>
    <PackageLicenseExpression></PackageLicenseExpression>
    <!--<PackageIcon>Barbarian King.png</PackageIcon>-->
    <RepositoryUrl>https://github.com/devhl-labs/CocApi</RepositoryUrl>
    <PackageTags>ClashOfClans SuperCell devhl</PackageTags>
    <PackageReleaseNotes>Fixed bug in player rankings</PackageReleaseNotes>
    <ApplicationIcon />
    <AssemblyName>CocApi</AssemblyName>
    <RootNamespace>CocApi</RootNamespace>
    <Company />
    <!--<PackageLicenseFile>LICENSE.md</PackageLicenseFile>-->
    <Title>CocApi</Title>
    <DocumentationFile>bin\$(Configuration)\$(TargetFramework)\CocApi.xml</DocumentationFile>
    <RepositoryType>git</RepositoryType>
    <PackageLicenseFile>LICENSE.md</PackageLicenseFile>
    <PackageIcon>Barbarian King.png</PackageIcon>

  </PropertyGroup>

  <ItemGroup>
    <!-- <PackageReference Include="JsonSubTypes" Version="1.7.0" /> -->
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    <PackageReference Include="RestSharp" Version="106.10.1" />
    <!-- <PackageReference Include="Polly" Version="7.2.0" /> -->
    <PackageReference Include="System.Collections.Immutable" Version="5.0.0" />
    <PackageReference Include="System.ComponentModel.Annotations" Version="4.7.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\Barbarian King.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="..\..\LICENSE.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>